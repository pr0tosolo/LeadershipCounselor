@prefix ex: <http://example.com/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

ex:Agent a owl:Class ;
    rdfs:label "Agent"@en ;
    rdfs:comment "Represents any party or organization that can participate in agreements." .

ex:Customer a owl:Class ;
    skos:prefLabel "Customer"@en ;
    skos:definition "An individual or organization with an account." ;
    rdfs:subClassOf ex:Agent .

ex:BusinessCustomer a owl:Class ;
    rdfs:label "Business Customer"@en ;
    rdfs:subClassOf ex:Agent .

ex:Account a owl:Class ;
    rdfs:label "Account"@en ;
    rdfs:comment "Represents an account." .

ex:DepositoryAccount a owl:Class ;
    rdfs:label "Depository Account"@en ;
    rdfs:subClassOf ex:Account .

ex:CheckingAccount a owl:Class ;
    rdfs:label "Checking Account"@en ;
    skos:definition "A bank account for day to day banking." ;
    rdfs:subClassOf ex:DepositoryAccount .

ex:Loan a owl:Class ;
    rdfs:label "Loan"@en .

ex:Product a owl:Class ;
    rdfs:label "Product"@en .

ex:Service a owl:Class ;
    rdfs:label "Service"@en .

ex:hasAccount a owl:ObjectProperty ;
    rdfs:label "has account" ;
    rdfs:domain ex:Customer ;
    rdfs:range ex:Account ;
    rdfs:subPropertyOf ex:relatedTo .

ex:relatedTo a owl:ObjectProperty .

ex:customer123 a owl:NamedIndividual ;
    rdf:type ex:Customer ;
    rdfs:label "Customer 123" .
